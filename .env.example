APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=daily,database
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

# Queue Worker Mode: 'cronjob' (default, simple) or 'supervisor' (production, continuous)
# See CRONJOB-SETUP.md for setup instructions
QUEUE_WORKER_MODE=cronjob

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

GEOCODE_KEY=
MAIL_INVOICE_ADDRESS=buchhaltung@ihre-domain.de
MAIL_SUPPORT_ADDRESS=support@ihre-domain.de

# Critical Log Notifications
# ERROR, CRITICAL, ALERT, EMERGENCY logs will be sent to MAIL_SUPPORT_ADDRESS
# Rate limited to prevent spam in case of cascading errors
# Leave MAIL_SUPPORT_ADDRESS empty to disable critical log notifications
CRITICAL_LOG_RATE_LIMIT=10
CRITICAL_LOG_DEDUPE_MINUTES=5
CRITICAL_LOG_QUEUE=true

RECAPTCHAV3_SITEKEY=
RECAPTCHAV3_SECRET=

# Keycloak SSO Configuration
KEYCLOAK_CLIENT_ID=
KEYCLOAK_CLIENT_SECRET=
KEYCLOAK_REDIRECT_URI="${APP_URL}/auth/keycloak/callback"
KEYCLOAK_BASE_URL=
KEYCLOAK_REALM=

